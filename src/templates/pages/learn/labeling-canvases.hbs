  fill(200,0,0);
  ellipse(40, 55, 18);
  ellipse(40, 45, 18);
  ellipse(50, 40, 18);
  ellipse(60, 55, 18);
  ellipse(60, 45, 18);
  ellipse(50, 60, 18);

  fill("#FFE200");
  ellipse(50,50,15);
}
</script>

