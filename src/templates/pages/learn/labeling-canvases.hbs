function setup() {
  createCanvas(100, 100);
}

function draw() {
  background(255,192,203);
  noStroke();
  fill(200,0,0);
  
  textSize(12);
  textStyle(NORMAL);
  describeElement("Text", "The text 'Hello world!' in the upper left corner of a pink canvas.'");
  text('Hello world!', 5, 30);
}
</script>

