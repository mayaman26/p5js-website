<script>
    $("#filters :checkbox").click(function() {

       var re = new RegExp($("#filters :checkbox:checked").map(function() {
                              return this.value;
                           }).get().join("|") ); //.join("|")
       $(".case-list").each(function() {
          var $this = $(this);
          $this[re.source!="" && re.test($this.attr("data-category")) ? "fadeIn" : "hide"]();
       });
    });
</script>


